{
    "manifest_version": 2,
    "name": "Youtube stream smoother",
    "version": "1.0",
    "description": "Skips video adds and removes popups",
    "icons": {
      "48": "icons/skip-48.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["yt_stream_smoother.js"]
      }
    ]
  }
  